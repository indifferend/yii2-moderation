{
    "name": "indifferend/yii2-moderation",
    "description": "A simple Content Moderation System for Yii2 that allows you to Approve or Reject resources like posts, comments, etc.",
    "type": "yii2-extension",
    "keywords": [
        "yii2",
        "moderation",
        "moderation-system",
        "content-moderation"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Igor Chepurnoi",
            "email": "chepurnoi.igor@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "yiisoft/yii2": "~2.0.10",
        "indifferend/yii2-enum": "*"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "indifferend\\moderation\\": ""
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
